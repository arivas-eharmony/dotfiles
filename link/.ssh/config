Host *

# Forward ssh agent to the remote machine.
ForwardAgent yes

# Automatically add all common hosts to the host file as they are connected to.
StrictHostKeyChecking no

# Github ssh account switching
# See .gitconfig alias
# http://arnaudrinquin.github.io/worflow/2014/03/11/fast-github-account-switch/

#Default GitHub
Host github.com
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_rsa

# Professional github alias
Host github_pro
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_rsa_pro
